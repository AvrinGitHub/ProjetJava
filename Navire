package projetjava;

/**
 *
 * @author Admin
 */
public abstract class Navire {
    //Taille du navire
    int taille = 0;
    
    public abstract void Tir(String navire);
    
    public abstract void Deplacement(String navire, String[][] Grille, int i, int j);
    
    public int getTaille(){
        return this.taille;
    }
}
