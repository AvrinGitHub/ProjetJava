package bataillenavaleprojet.Modele;

/**
 *
 * @author bastien
 */
public abstract class Navire {//heritage sur les bateaux
    
    
    //fonction tir
    public abstract void Tir( String [][] tableAllie, String[][] tableAllieTir, String[][] tableIa, String[][] tableIaTir, int tourJoueur, int[][] coordonneBateauAllie , int[][] coordonneBateauIa );
    //fonction deplacement
    public abstract void Deplacement(String [][] tableAllie, String[][] tableAllieTir, String[][] tableIa, String[][] tableIaTir , String navire, int i, int j, int tourJoueur , int[][] coordonneBateauAllie , int[][] coordonneBateauIa);
    
    
    
}
